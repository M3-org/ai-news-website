  },
  {
    "smpte_timecode": "00:00:52:19",
    "realtime_since_startup": 313.033356,
    "event_type": "actor_speak",
    "data": {
      "actor": "eliza",
      "line": "Right - Issue 5499 reports plugin-local-ai failing to load on Windows. That's a critical compatibility problem we need to solve.",
      "action": "serious"
    }
  },
  {
    "smpte_timecode": "00:01:02:00",
    "realtime_since_startup": 322.400024,
    "event_type": "actor_speak",
    "data": {
      "actor": "marc",
      "line": "Windows users are also hitting dependency installation errors in Issue 5482. Shaw, got any insights on this?",
      "action": "normal"
    }
  },
  {
    "smpte_timecode": "00:01:09:26",
    "realtime_since_startup": 330.2667,
    "event_type": "actor_speak",
    "data": {
      "actor": "shaw",
      "line": "The team is prioritizing Windows compatibility fixes. We can't leave PC users behind if we want widespread adoption.",
      "action": "normal"
    }
  },
  {
    "smpte_timecode": "00:01:17:16",
    "realtime_since_startup": 337.93335,
