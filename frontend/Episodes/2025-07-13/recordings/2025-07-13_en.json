    "data": {
      "location": "splitscreen"
    }
  },
  {
    "smpte_timecode": "00:01:17:19",
    "realtime_since_startup": 338.033356,
    "event_type": "actor_speak",
    "data": {
      "actor": "shaw",
      "line": "Let me show you some of our exciting new features. First up is our Forms plugin.",
      "action": "excited"
    }
  },
  {
    "smpte_timecode": "00:01:22:07",
    "realtime_since_startup": 342.633362,
    "event_type": "actor_speak",
    "data": {
      "actor": "shaw",
      "line": "This allows other plugins to create dynamic user interfaces on the fly.",
      "action": "normal"
    }
  },
  {
    "smpte_timecode": "00:01:26:22",
    "realtime_since_startup": 347.133362,
    "event_type": "actor_speak",
    "data": {
      "actor": "shaw",
      "line": "But what's really game-changing is the new action chaining capability.",
      "action": "excited"
