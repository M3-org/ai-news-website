  },
  {
    "smpte_timecode": "00:00:33:01",
    "realtime_since_startup": 293.43335,
    "event_type": "actor_speak",
    "data": {
      "actor": "marc",
      "line": "Indeed! Better testing tools mean better plugins. And better plugins mean... beep boop... superior artificial intelligence!",
