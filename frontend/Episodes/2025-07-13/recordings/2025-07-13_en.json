  },
  {
    "smpte_timecode": "00:01:30:20",
    "realtime_since_startup": 351.066681,
    "event_type": "actor_speak",
    "data": {
      "actor": "shaw",
      "line": "Imagine connecting multiple AI behaviors seamlessly - that's what action chaining enables.",
      "action": "normal"
    }
  },
  {
    "smpte_timecode": "00:01:36:08",
    "realtime_since_startup": 356.6667,
    "event_type": "actor_speak",
    "data": {
      "actor": "marc",
      "line": "The implications for workflow automation are massive!",
      "action": "excited"
    }
  },
  {
