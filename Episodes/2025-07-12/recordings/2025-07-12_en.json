  },
  {
    "smpte_timecode": "00:03:14:09",
    "realtime_since_startup": 500.400024,
    "event_type": "actor_speak",
    "data": {
      "actor": "shaw",
      "line": "We're actively working on resolving the ESM URL scheme errors affecting plugin-local-ai.",
      "action": "serious"
