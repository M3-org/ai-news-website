    "realtime_since_startup": 488.7667,
    "event_type": "actor_speak",
    "data": {
      "actor": "shaw",
      "line": "It allows for sequential actions with state storage between steps, enabling more sophisticated agent behaviors.",
      "action": "technical"
    }
  },
  {
    "smpte_timecode": "00:03:09:14",
    "realtime_since_startup": 495.566681,
    "event_type": "actor_speak",
    "data": {
