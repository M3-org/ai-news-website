    "event_type": "actor_speak",
    "data": {
      "actor": "shaw",
      "line": "The action chaining feature is a game-changer for complex agent workflows.",
      "action": "excited"
    }
  },
  {
    "smpte_timecode": "00:03:00:09",
    "realtime_since_startup": 486.400024,
    "event_type": "actor_speak",
    "data": {
      "actor": "eliza",
      "line": "How exactly does it work, Shaw?",
      "action": "curious"
    }
