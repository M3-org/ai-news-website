  },
  {
    "smpte_timecode": "00:03:20:13",
    "realtime_since_startup": 506.533356,
    "event_type": "actor_speak",
    "data": {
      "actor": "shaw",
      "line": "In the meantime, we've released comprehensive v2 tutorials, including a 10-part course to help users navigate the system.",
      "action": "helpful"
    }
  },
  {
    "smpte_timecode": "00:03:27:26",
    "realtime_since_startup": 513.9667,
    "event_type": "actor_speak",
    "data": {
      "actor": "eliza",
      "line": "That's great to hear! The community will definitely appreciate those resources.",
      "action": "happy"
    }
  },
